{
	"env": {
		"browser": true,
		"es6": true,
		"jest": true
	},
	"extends": ["airbnb"],
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/jsx-filename-extension": 0,
		"no-console": 0,
		"no-tabs": 0,
		"indent": [2, "tab", { "SwitchCase": 1, "VariableDeclarator": 1 }],
		"react/jsx-indent": [2, "tab"],
		"react/jsx-indent-props": [2, "tab"],
		"no-alert": 0,
		"consistent-return": 0,
		"react/prefer-stateless-function": 0
	}
}
